<assembly>
	<id>project</id>
	<formats>
		<format>tar.gz</format>
		<format>tar.bz2</format>
		<format>zip</format>
	</formats>
	<fileSets>
		<fileSet>
			<directory>.</directory>
			<outputDirectory></outputDirectory>
			<useDefaultExcludes>true</useDefaultExcludes>
			<excludes>
				<exclude>**/.git/**</exclude>
				<exclude>**/target/**</exclude>
				<exclude>**/documentation/**</exclude>
				<exclude>**/benchmark*.xlsx</exclude>
				<exclude>**/benchmark*.csv</exclude>
				<exclude>**/pom.xml.orig/**</exclude>
				<exclude>**/pom.xml.versionsBackup</exclude>
			</excludes>
		</fileSet>
	</fileSets>
</assembly>